FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 4.69014022557838E-005;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 4.69014022557838E-005;
    }
    outlet
    {
        type            zeroGradient;
    }
    upperwall
    {
        type            symmetry;
    }
    lowerwall
    {
        type            symmetry;
    }
    pipe1
    {
        type            epsilonWallFunction;
        value           uniform 4.69014022557838E-005;
    }
    pipe2
    {
        type            epsilonWallFunction;
        value           uniform 4.69014022557838E-005;
    }
    frontandback
    {
        type            empty;
    }
}


